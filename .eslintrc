{
  "globals": {
    "expect": true
  },
  "plugins": [
    "markdown"
  ],
  "extends": [
    "prettier",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "env": {
    "node": true,
     "es6": true
  },
  "rules": {
    "linebreak-style": ["error", "unix"],
    "max-len": ["error", { "code": 120 , "ignoreStrings": true, "ignoreTemplateLiterals": true}]
  }
}
